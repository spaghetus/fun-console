	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'fun'",
			"cargo": {
				"args": [
					"build",
					"--bin=fun",
					"--package=fun"
				],
				"filter": {
					"name": "fun",
					"kind": "bin"
				}
			},
			"args": [
				"examples",
				"mutation"
			],
			"cwd": "${workspaceFolder}"
		}
	]
}